{
  "name": "sequelize-with-postgres",
  "version": "1.0.0",
  "main": "index.js",
  "author": "orie chinedu",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrate:remove": "npx sequelize-cli db:migrate:undo:all",
    "seeders": "npx sequelize-cli db:seed:all",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "coverage": "npm run pretest && jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.12.3",
    "sequelize-cli": "^5.5.0",
    "swagger-express-middleware": "^2.0.4",
    "@okta/jwt-verifier": "0.0.11",
    "@okta/okta-sdk-nodejs": "^2.0.0",
    "babel-preset-airbnb": "^3.2.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "dotenv-extended": "^2.4.0",
    "ejs": "^2.6.1",
    "eureka-js-client": "^4.4.2",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "ngrok": "^3.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "swagger-jsdoc": "^3.2.9",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  }
}
